<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<mapping map-id="MAPPING_USER_LOGIN" map-empty-string="false"
		map-null="false" wildcard="false">
		<class-a>org.concourplus.model.usersetup.User</class-a>
		<class-b>org.concourplus.dto.usersetup.UserDTO</class-b>
	</mapping>
	<mapping map-id="MAPPING_USER_AUTHENTICATION"
		map-empty-string="false" map-null="false" wildcard="false">
		<class-a>org.concourplus.model.usersetup.User</class-a>
		<class-b>org.concourplus.dto.usersetup.UserDTO</class-b>
		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>firstName</a>
			<b>firstName</b>
		</field>
		<field>
			<a>lastName</a>
			<b>lastName</b>
		</field>
		<field>
			<a>username</a>
			<b>username</b>
		</field>
		<field>
			<a>token</a>
			<b>token</b>
		</field>
		<field>
			<a>tokenDate</a>
			<b>tokenDate</b>
		</field>
	</mapping>

</mappings>